<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
    <menu id="root-menu" label="Openbox 3">
        <item label="Run Program">
            <action name="Execute">
                <execute>
                    gmrun
                </execute>
            </action>
        </item>
        <separator/>
        <item label="X-kill">
            <action name="Execute">
                <execute>
                    xkill
                </execute>
            </action>
        </item>
        <separator/>
        <item label="Leafpad">
            <action name="Execute">
                <execute>
                    leafpad
                </execute>
            </action>
        </item>
        <menu id="system" label="System">
            <item label="Lxrandr">
                <action name="Execute">
                    <execute>
                        lxrandr
                    </execute>
                </action>
            </item>
            <item label="Nitrogen">
                <action name="Execute">
                    <execute>
                        nitrogen
                    </execute>
                </action>
            </item>
        </menu>
        <separator/>
        <item label="Exit">
            <action name="Execute">
                <execute>
                    <!-- openbox-logout -->
                    oblogout
                </execute>
            </action>
        </item>
    </menu>
</openbox_menu>
